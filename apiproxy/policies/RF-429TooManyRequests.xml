<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-429TooManyRequests">
    <DisplayName>RF-429TooManyRequests</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">{ "error" : "Too Many Requests. Wait few seconds and try again." }</Payload>
            <StatusCode>429</StatusCode>
            <ReasonPhrase>Too Many Requests</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>